{
  "name": "proc-that",
  "version": "0.2.5",
  "description": "proc(ess)-that - easy extendable etl tool for nodejs written in typesript",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf build",
    "pretest": "npm install && typings install && npm run clean && tsc",
    "test": "mocha --ui bdd --recursive ./build",
    "develop": "npm run clean && tsc -w",
    "build": "rimraf build dist && tsc --outDir dist -d && del './dist/**/*.spec*'"
  },
  "keywords": [
    "etl",
    "node",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:buehler/proc-that.git"
  },
  "bugs": "https://github.com/buehler/proc-that/issues",
  "author": "Christoph BÃ¼hler <christoph.buehler@bluewin.ch>",
  "license": "MIT",
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "del-cli": "^0.2.0",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "tslint": "^3.7.1",
    "typescript": "^1.8.9",
    "typings": "^0.7.11"
  },
  "dependencies": {
    "rxjs": "5.0.0-beta.4"
  }
}
